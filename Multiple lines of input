first line input: 5 , 5
second line input: 1 2 3 4 5
third line input: 5 6 7 8 9

#include <iostream>
#include <vector>

int main() {
  int m = 0, n = 0, j;
  std::vector<int> input_m(5, 0);
  std::vector<int> input_n(5, 0);
  std::cin >> m >> n;
  std::cout << m << " " << n << std::endl;
  int count_m = 0, count_n = 0;
  while (count_m < m) {
    scanf("%d", &input_m[count_m++]);
  }
  while (count_n < n) {
    scanf("%d", &input_n[count_n++]);
  }

  for(auto i: input_m)
    std::cout << i << " ";
  std::cout << std::endl;
  for(auto i: input_n)
    std::cout << i << " ";
  return 0;
}
